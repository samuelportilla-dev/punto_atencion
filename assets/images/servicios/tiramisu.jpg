<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="plateGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8f8f8;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e8e8e8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="tiramisuGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F5DEB3;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#DEB887;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="cocoaGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B4513;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#654321;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Plato -->
  <ellipse cx="200" cy="200" rx="180" ry="60" fill="url(#plateGrad)" stroke="#d0d0d0" stroke-width="2"/>
  
  <!-- Tiramisú -->
  <rect x="120" y="140" width="160" height="60" rx="10" fill="url(#tiramisuGrad)" stroke="#D2691E" stroke-width="2"/>
  
  <!-- Capas del tiramisú -->
  <rect x="120" y="140" width="160" height="15" fill="#F5F5DC"/>
  <rect x="120" y="155" width="160" height="15" fill="url(#tiramisuGrad)"/>
  <rect x="120" y="170" width="160" height="15" fill="#F5F5DC"/>
  <rect x="120" y="185" width="160" height="15" fill="url(#tiramisuGrad)"/>
  
  <!-- Cacao en polvo -->
  <ellipse cx="200" cy="150" rx="60" ry="8" fill="url(#cocoaGrad)" opacity="0.8"/>
  
  <!-- Decoración con cacao -->
  <circle cx="160" cy="145" r="3" fill="url(#cocoaGrad)"/>
  <circle cx="180" cy="147" r="2" fill="url(#cocoaGrad)"/>
  <circle cx="200" cy="145" r="3" fill="url(#cocoaGrad)"/>
  <circle cx="220" cy="147" r="2" fill="url(#cocoaGrad)"/>
  <circle cx="240" cy="145" r="3" fill="url(#cocoaGrad)"/>
  
  <!-- Hojas de menta -->
  <path d="M170 135 Q175 130 180 135 Q185 130 190 135" fill="#228B22" stroke="#006400" stroke-width="1"/>
  <path d="M210 137 Q215 132 220 137 Q225 132 230 137" fill="#228B22" stroke="#006400" stroke-width="1"/>
  
  <!-- Título -->
  <text x="200" y="50" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="24" font-weight="bold">Tiramisú</text>
</svg>
