<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="plateGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8f8f8;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e8e8e8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="steakGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B4513;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#A0522D;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Plato -->
  <ellipse cx="200" cy="200" rx="180" ry="60" fill="url(#plateGrad)" stroke="#d0d0d0" stroke-width="2"/>
  
  <!-- Filete de res -->
  <ellipse cx="200" cy="170" rx="80" ry="30" fill="url(#steakGrad)" stroke="#654321" stroke-width="2"/>
  
  <!-- Líneas de parrilla -->
  <line x1="140" y1="150" x2="260" y2="150" stroke="#654321" stroke-width="1" opacity="0.6"/>
  <line x1="140" y1="160" x2="260" y2="160" stroke="#654321" stroke-width="1" opacity="0.6"/>
  <line x1="140" y1="170" x2="260" y2="170" stroke="#654321" stroke-width="1" opacity="0.6"/>
  <line x1="140" y1="180" x2="260" y2="180" stroke="#654321" stroke-width="1" opacity="0.6"/>
  <line x1="140" y1="190" x2="260" y2="190" stroke="#654321" stroke-width="1" opacity="0.6"/>
  
  <!-- Guarnición - Papas -->
  <ellipse cx="120" cy="190" rx="15" ry="8" fill="#DEB887"/>
  <ellipse cx="100" cy="195" rx="12" ry="6" fill="#DEB887"/>
  <ellipse cx="140" cy="195" rx="10" ry="5" fill="#DEB887"/>
  
  <!-- Guarnición - Vegetales -->
  <circle cx="280" cy="185" r="8" fill="#90EE90"/>
  <circle cx="300" cy="190" r="6" fill="#90EE90"/>
  <circle cx="290" cy="200" r="7" fill="#90EE90"/>
  
  <!-- Salsa -->
  <ellipse cx="200" cy="200" rx="40" ry="8" fill="#8B0000" opacity="0.7"/>
  
  <!-- Perejil decorativo -->
  <path d="M180 160 Q185 155 190 160" fill="#228B22" stroke="#006400" stroke-width="1"/>
  <path d="M210 162 Q215 157 220 162" fill="#228B22" stroke="#006400" stroke-width="1"/>
  
  <!-- Título -->
  <text x="200" y="50" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="24" font-weight="bold">Filete de Res</text>
</svg>
